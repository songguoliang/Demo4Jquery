<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<servlet>
		<servlet-name>remote</servlet-name>
		<servlet-class>com.songguoliang.servlet.RemoteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>remote</servlet-name>
		<url-pattern>/remote.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>auto</servlet-name>
		<servlet-class>com.songguoliang.servlet.AutocompleteServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>auto</servlet-name>
		<url-pattern>/auto.do</url-pattern>
	</servlet-mapping>
  <welcome-file-list>
  	<welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
